{
    "env": {
        "es6": true
    },
    "extends": [
      "standard",
      "plugin:react/recommended"
    ],
    "globals": {
        "use": true,
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}
